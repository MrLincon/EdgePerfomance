<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.edgeperformance.edge">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.EdgePerformance">
        <activity android:name="io.edgeperformance.edge.Authentication.SignInActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="io.edgeperformance.edge.Authentication.SignUpActivity"></activity>
        <activity android:name="io.edgeperformance.edge.Authentication.PasswordRecoveryActivity"></activity>
        <activity android:name="io.edgeperformance.edge.Authentication.InformationActivity"></activity>
        <activity android:name="io.edgeperformance.edge.Activities.SettingsActivity"
            android:parentActivityName="io.edgeperformance.edge.Activities.MainActivity"></activity>
        <activity
            android:name="io.edgeperformance.edge.Activities.MainActivity"
            android:exported="true">

        </activity>
        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>